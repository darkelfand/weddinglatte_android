<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.redborn.weddinglatte">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".android.IntroActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".android.view.MainActivity" android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".android.view.LoginActivity" />
        <activity android:name=".android.view.JoinActivity" />
        <activity android:name=".android.view.MyPageActivity" />
        <activity android:name=".android.view.TermArgeeActivity" />
    </application>

</manifest>